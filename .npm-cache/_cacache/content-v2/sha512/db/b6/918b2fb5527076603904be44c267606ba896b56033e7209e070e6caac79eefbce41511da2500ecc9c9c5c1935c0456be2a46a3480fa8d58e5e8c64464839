emarkGfm from 'remark-gfm';\n\nmodule.exports = {\n  module: {\n    rules: [\n      {\n        test: /\\.(stories|story)\\.mdx$/,\n        use: [\n          {\n            loader: require.resolve('@storybook/mdx2-csf/loader'),\n            options: {\n              skipCsf: false,\n              mdxCompileOptions: {\n                remarkPlugins: [remarkGfm],\n              },\n            },\n          },\n        ],\n      },\n    ],\n  },\n};\n```\n\n## Contributing\n\nWe welcome contributions to Storybook!\n\n- ðŸ“¥ Pull requests and ðŸŒŸ Stars are always welcome.\n- Read our [contributing guide](CONTRIBUTING.md) to get started,\n  or find us on [Discord](https://discord.gg/storybook), we will take the time to guide you\n\n## License\n\n[MIT](https://github.com/storybookjs/csf-mdx2/blob/main/LICENSE)\n","gitHead":"7ccaa6274cee4d05c7908d4fc11c686bf7ad4efd","scripts":{"test":"jest","build":"tsup","start":"yarn build && yarn storybook -- --no-manager-cache --quiet\"","prepare":"husky install","release":"yarn build && auto shipit","prettier":"prettier","storybook":"storybook dev -p 6006","build-storybook":"storybook build"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/csf-mdx2.git","type":"git"},"_npmVersion":"6.14.17","description":"MDXv2 to CSF webpack compiler and loader","directories":{},"lint-staged":{"*.{ts,js,css,md}":"prettier --write"},"_nodeVersion":"14.21.1","dependencies":{"loader-utils":"^2.0.4"},"publishConfig":{"access":"public"},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"auto":"^10.3.0","jest":"^27.0.6","tsup":"^6.2.2","husky":">=6","react":"^17.0.1","lodash":"^4.17.21","rimraf":"^3.0.2","prompts":"^2.4.2","ts-jest":"^27.0.4","prettier":"^2.3.1","react-dom":"^17.0.1","storybook":"next","ts-dedent":"^2.2.0","@babel/cli":"^7.12.1","babel-jest":"^27.0.6","remark-gfm":"^3.0.1","typescript":"^4.2.4","@babel/core":"^7.12.3","@jest/types":"^27.0.6","@mdx-js/mdx":"^2.0.0","@types/jest":"^27.0.3","@types/node":"^16.4.1","lint-staged":">=10","@babel/types":"^7.14.8","babel-loader":"^8.1.0","concurrently":"^7.0.0","@babel/parser":"^7.12.11","@types/lodash":"^4.14.167","@babel/template":"^7.14.5","@babel/traverse":"^7.12.11","estree-to-babel":"^4.9.0","@babel/generator":"^7.12.11","@storybook/react":"next","js-string-escape":"^1.0.1","@babel/preset-env":"^7.12.1","@babel/preset-react":"^7.12.5","hast-util-to-estree":"^2.0.2","@testing-library/dom":"^8.1.0","@storybook/addon-links":"next","@testing-library/react":"^12.0.0","jest-environment-jsdom":"^27.0.6","@types/js-string-escape":"^1.0.1","@babel/preset-typescript":"^7.13.0","@storybook/addon-actions":"next","@storybook/react-webpack5":"next","@storybook/testing-library":"next","@storybook/addon-essentials":"next","@testing-library/user-event":"^13.2.1","@storybook/addon-interactions":"next"},"_npmOperationalInternal":{"tmp":"tmp/mdx2-csf_0.1.0-next.8_1670471972740_0.7815272222280067","host":"s3://npm-registry-packages"}},"0.0.4-canary.30.c978d7f.0":{"name":"@storybook/mdx2-csf","version":"0.0.4-canary.30.c978d7f.0","author":{"name":"Michael Shilman","email":"michael@lab80.co"},"license":"MIT","_id":"@storybook/mdx2-csf@0.0.4-canary.30.c978d7f.0","maintainers":[{"name":"kylegach","email":"kyle.gach@gmail.com"},{"name":"tooppaaa","email":"clementdungler@gmail.com"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"shilman","email":"michael@lab80.co"},{"name":"alexandrebodin","email":"bodin.alex@gmail.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"danielduan","email":"dduan@yahoo.com"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"pksunkara","email":"pavan.sss1991@gmail.com"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"gongreg","email":"gytis.vinclovas@gmail.com"},{"name":"domyen","email":"dom@hichroma.com"},{"name":"usulpro","email":"regx@usul.su"},{"name":"kylesuss","email":"susskyle@gmail.com"},{"name":"ghengeveld","email":"