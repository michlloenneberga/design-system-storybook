 addon:\n\n```js\nimport { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","gitHead":"9de487fa7b79cef77fd8de0d0357e8cf962dc776","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v12.19.0+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"12.19.0","dependencies":{"core-js":"^3.0.1"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.0.0","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.2.0-alpha.13_1610532542304_0.5397149863615767","host":"s3://npm-registry-packages"}},"6.2.0-alpha.14":{"name":"@storybook/postinstall","version":"6.2.0-alpha.14","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.2.0-alpha.14","maintainers":[{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"shilman","email":"michael@lab80.co"},{"name":"alexandrebodin","email":"bodin.alex@gmail.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"danielduan","email":"dduan@yahoo.com"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"pksunkara","email":"pavan.sss1991@gmail.com"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"gongreg","email":"gytis.vinclovas@gmail.com"},{"name":"domyen","email":"dom@hichroma.com"},{"name":"usulpro","email":"regx@usul.su"},{"name":"kylesuss","email":"susskyle@gmail.com"},{"name":"ghengeveld","email":"info@ghengeveld.nl"}