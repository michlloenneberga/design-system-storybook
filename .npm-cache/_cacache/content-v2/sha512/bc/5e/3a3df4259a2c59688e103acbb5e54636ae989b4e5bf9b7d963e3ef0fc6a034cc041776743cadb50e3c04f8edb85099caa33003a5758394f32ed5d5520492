esets.js` in your addon:\n\n```js\nimport { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","bundler":{"entries":["./src/index.ts"]},"exports":{".":{"node":"./dist/index.js","types":"./dist/index.d.ts","import":"./dist/index.mjs","require":"./dist/index.js"},"./package.json":"./package.json"},"funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"e7c833e690dd0d2966ffb7fb806a3bda89a508c8","scripts":{"prep":"../../../scripts/prepare/bundle.ts","check":"../../../scripts/node_modules/.bin/tsc --noEmit"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/6.6.1/node@v16.17.0+arm64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"16.17.0","publishConfig":{"access":"public"},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^29.3.1","typescript":"~4.9.3","jscodeshift":"^0.14.0","jest-specific-snapshot":"^7.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_7.1.0-alpha.10_1682621484746_0.7152269371904589","host":"s3://npm-registry-packages"}},"7.1.0-alpha.11":{"name":"@storybook/postinstall","version":"7.1.0-alpha.11","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@7.1.0-alpha.11","maintainers":[{"name":"shilman","email":"michael@lab80.co"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"}],"homepage":"https://github.com/storybookjs/storybook/tree/main/lib/postinstall","bugs":{"url":"https://github.c