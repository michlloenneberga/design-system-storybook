shift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","gitHead":"cc1c152e5452d28eabb24330f4ed693cd6782bc1","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.0/node@v12.16.2+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"_nodeVersion":"12.16.2","dependencies":{"core-js":"^3.0.1"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"*":["ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.0.0","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.1.0-alpha.20_1601874946442_0.90317396591667","host":"s3://npm-registry-packages"}},"6.0.26":{"name":"@storybook/postinstall","version":"6.0.26","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.0.26","maintainers":[{"name":"dannyhw","email":"danny.h.williams@hotmail.com"},{"name":"mrmckeb","email":"mrmckeb.npm@outlook.com"},{"name":"codebyalex","email":"wilsonc.alexander@gmail.com"},{"name":"dandean","email":"me@dandean.com"},{"name":"pago","email":"patrick.gotthardt@trivago.com"},{"name":"ghengeveld","email":"info@ghengeveld.nl"},{"name":"kylesuss","email":"susskyle@gmail.com"},{"name":"usulpro","email":"regx@usul.su"},{"name":"domyen","email":"dom@hichroma.com"},{"name":"gongreg","email":"gytis.vinclovas@gmail.com"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"pksunkara","email":"pavan.sss1991@gmail.com"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"shilman","email":"michael@lab80.co"},{"name":"alexandrebodin","email":"bodin.alex@gmail.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"danielduan","email":"dduan@yah