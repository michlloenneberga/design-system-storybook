eset to `presets.js`, simply create a file `postinstall/presets.js` in your addon:\n\n```js\nimport { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"e88f5c202decc2e594d08266d41b2b561e0ceb42","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"sbmodern":"dist/modern/index.js","repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v14.16.1+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"14.16.1","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.3.0-alpha.34_1621388572575_0.08965581384818444","host":"s3://npm-registry-packages"}},"6.3.0-alpha.35":{"name":"@storybook/postinstall","version":"6.3.0-alpha.35","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.3.0-alpha.35","maintainers":[{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"shilman","email":"michael@lab80.co"}],"homepage":"https://github.com/storybookjs/storybook/tree/master/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"a7a8d7a5318e6874af96d2cb12fa7fc36e74be12","t