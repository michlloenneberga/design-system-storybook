 Adding a preset\n\nTo add a preset to `presets.js`, simply create a file `postinstall/presets.js` in your addon:\n\n```js\nimprot { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","gitHead":"0e4076e643e32f690bf9228a0430faa0603e75c8","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.20.2/node@v12.16.2+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"_nodeVersion":"12.16.2","dependencies":{"core-js":"^3.0.1"},"publishConfig":{"access":"public"},"typesVersions":{"<=3.5":{"*":["ts3.5/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.0.0","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^3.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.0.0-beta.4_1588943070127_0.0917317765573078","host":"s3://npm-registry-packages"}},"6.0.0-beta.5":{"name":"@storybook/postinstall","version":"6.0.0-beta.5","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.0.0-beta.5","maintainers":[{"name":"alexandrebodin","email":"bodin.alex@gmail.com"},{"name":"codebyalex","email":"wilsonc.alexander@gmail.com"},{"name":"dandean","email":"me@dandean.com"},{"name":"danielduan","email":"dduan@yahoo.com"},{"name":"domyen","email":"dom@hichroma.com"},{"name":"ghengeveld","email":"info@ghengeveld.nl"},{"name":"gongreg","email":"gytis.vinclovas@gmail.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"kylesuss","email":"susskyle@gmail.com"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"pa