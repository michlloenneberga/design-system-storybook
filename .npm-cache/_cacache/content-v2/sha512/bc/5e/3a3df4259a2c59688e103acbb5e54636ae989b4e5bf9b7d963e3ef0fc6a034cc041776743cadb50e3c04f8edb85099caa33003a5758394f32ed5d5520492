.js` in your addon:\n\n```js\nimprot { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","gitHead":"870ff062f31a5140b82148dd453326f7ba9f76ad","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.20.2/node@v10.16.3+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"_nodeVersion":"10.16.3","dependencies":{"core-js":"^3.0.1"},"publishConfig":{"access":"public"},"typesVersions":{"<=3.5":{"*":["ts3.5/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^25.1.0","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^2.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.0.0-alpha.26_1584017935504_0.5687984493347547","host":"s3://npm-registry-packages"}},"6.0.0-alpha.27":{"name":"@storybook/postinstall","version":"6.0.0-alpha.27","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.0.0-alpha.27","maintainers":[{"name":"alexandrebodin","email":"bodin.alex@gmail.com"},{"name":"dandean","email":"me@dandean.com"},{"name":"danielduan","email":"dduan@yahoo.com"},{"name":"domyen","email":"dom@hichroma.com"},{"name":"ghengeveld","email":"info@ghengeveld.nl"},{"name":"gongreg","email":"gytis.vinclovas@gmail.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"kylesuss","email":"susskyle@gmail.com"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"pago","email":"patrick.gotthardt@trivago.com"},{"name":"pksunkara","email":"pavan.sss1991@gmail.com"},{"name":"shilman","email":"michael@lab80.co"},