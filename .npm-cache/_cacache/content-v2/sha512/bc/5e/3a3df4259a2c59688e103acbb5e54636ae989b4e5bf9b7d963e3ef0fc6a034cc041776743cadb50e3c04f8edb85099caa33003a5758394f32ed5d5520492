 api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","gitHead":"c64a498a62180619dccf905a25346cc3e00247af","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.19.0/node@v10.16.3+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"_nodeVersion":"10.16.3","dependencies":{"core-js":"^3.0.1"},"publishConfig":{"access":"public"},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^2.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_5.3.0-rc.1_1577092932422_0.8778659934515358","host":"s3://npm-registry-packages"}},"5.3.0-rc.2":{"name":"@storybook/postinstall","version":"5.3.0-rc.2","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@5.3.0-rc.2","maintainers":[{"name":"alexandrebodin","email":"bodin.alex@gmail.com"},{"name":"danielduan","email":"dduan@yahoo.com"},{"name":"domyen","email":"dom@hichroma.com"},{"name":"ghengeveld","email":"info@ghengeveld.nl"},{"name":"gongreg","email":"gytis.vinclovas@gmail.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"kylesuss","email":"susskyle@gmail.com"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"pksunkara","email":"pavan.sss1991@gmail.com"},{"name":"shilman","email":"michael@lab80.co"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"usulpro","email":"regx@usul.su"}],"homepage":"https://github.com/storybookjs/storybook/tree/master/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"a39b70b3cf0e09d21139c9782c001012f63e2e2c","tarball":"https: