eset\n\nTo add a preset to `presets.js`, simply create a file `postinstall/presets.js` in your addon:\n\n```js\nimport { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","bundler":{"entries":["./src/index.ts"]},"exports":{".":{"node":"./dist/index.js","types":"./dist/index.d.ts","import":"./dist/index.mjs","require":"./dist/index.js"},"./package.json":"./package.json"},"funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"18dcfa6073157150da60c328f6f64abd477ddcad","scripts":{"prep":"../../../scripts/prepare/bundle.ts","check":"../../../scripts/node_modules/.bin/tsc --noEmit"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/6.6.1/node@v16.17.0+arm64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"16.17.0","publishConfig":{"access":"public"},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^29.3.1","typescript":"~4.9.3","jscodeshift":"^0.14.0","jest-specific-snapshot":"^7.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_7.1.0-alpha.4_1681491893661_0.08061465755503727","host":"s3://npm-registry-packages"}},"7.0.5":{"name":"@storybook/postinstall","version":"7.0.5","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@7.0.5","maintainers":[{"name":"shilman","email":"michael@lab80.co"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"}],"homepage":"https://github.com/storybookjs/storybook/tree/mai