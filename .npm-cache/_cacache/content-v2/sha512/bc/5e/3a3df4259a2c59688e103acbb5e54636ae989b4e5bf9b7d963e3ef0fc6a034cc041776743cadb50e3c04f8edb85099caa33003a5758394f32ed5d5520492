nimport { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","gitHead":"b100a45b69db3c2b9c4cd06e68d9035a769f592d","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v12.21.0+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"12.21.0","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.2.0-rc.9_1616604306665_0.0563716667849159","host":"s3://npm-registry-packages"}},"6.2.0-rc.10":{"name":"@storybook/postinstall","version":"6.2.0-rc.10","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.2.0-rc.10","maintainers":[{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"shilman","email":"michael@lab80.co"}],"homepage":"https://github.com/storybookjs/storybook/tree/master/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"fc15f2c866e002391c683bcaee245d2586401691","tarball":"https://registry.npmjs.org/@storybook/postinstall/-/postinstall-6.2.0-rc.10.tgz","fileCount":19,"integrity":"sha512-Svl4kuxRpLZLSZMoTrb0iWbtzAiVBdMGESjazCkUNEq7j25QFlRSGh6pUxv5EDHOYGW4Ii1rnswM58z4D4xYTg==","signa