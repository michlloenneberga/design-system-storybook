nst root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"1b43aa79911d0d9d936571533cbf76c6ecf928b0","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"sbmodern":"dist/modern/index.js","repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v15.14.0+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"15.14.0","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.4.0-alpha.10_1625226182026_0.9417633395465006","host":"s3://npm-registry-packages"}},"6.4.0-alpha.11":{"name":"@storybook/postinstall","version":"6.4.0-alpha.11","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.4.0-alpha.11","maintainers":[{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"shilman","email":"michael@lab80.co"}],"homepage":"https://github.com/storybookjs/storybook/tree/master/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"8086e2b70db27fb3bc115a8d6ce6d7c919a8d556","tarball":"https://registry.npmjs.org/@storybook/postinstall/-/postinstall-6.4.0-alpha.11.tgz","fileCount":24,"integrity":"sha512-wQc5K00A1SqRiJqSqNBQw1sDB8Acr6Q9oKZxWOVptbybxoW11PonE9K6/aJPXmls/e9a5Ho6JuREWAnqlVDXzw