nsformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"b2e85f3599c043815b18bbfc5ca563bf1c6e4add","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"sbmodern":"dist/modern/index.js","repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v16.13.0+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"16.13.0","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"dist/ts3.9/*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","jscodeshift":"^0.13.1","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.5.0-beta.0_1650781001506_0.5436485053223106","host":"s3://npm-registry-packages"}},"6.5.0-beta.1":{"name":"@storybook/postinstall","version":"6.5.0-beta.1","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.5.0-beta.1","maintainers":[{"name":"shilman","email":"michael@lab80.co"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"}],"homepage":"https://github.com/storybookjs/storybook/tree/main/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"2eb6f268d2a85e50878832c4f2bed4533653daaf","tarball":"https://registry.npmjs.org/@storybook/postinstall/-/postinstall-6.5.0-beta.1.tgz","fileCount":24,"integrity":"sha512-3v8PhlsSBz5AVjG/MPwah5hSZi7ZEwQR2nVzqvSPEEvC7kMht98oYwSoPFeV3NJoJI4I3M