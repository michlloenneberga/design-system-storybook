ck with jscodeshift internals.\n\n## Adding a preset\n\nTo add a preset to `presets.js`, simply create a file `postinstall/presets.js` in your addon:\n\n```js\nimport { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"a3d63d6a3fe28b6e3577ba9e50370a0d14ceca7e","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v15.14.0+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"15.14.0","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.3.0-alpha.23_1620714508363_0.4536943487266609","host":"s3://npm-registry-packages"}},"6.3.0-alpha.24":{"name":"@storybook/postinstall","version":"6.3.0-alpha.24","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.3.0-alpha.24","maintainers":[{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"shilman","email":"michael@lab80.co"}],"homepage":"https://github.com/storybookjs/storybook/tree/master/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"4c56e209c04f