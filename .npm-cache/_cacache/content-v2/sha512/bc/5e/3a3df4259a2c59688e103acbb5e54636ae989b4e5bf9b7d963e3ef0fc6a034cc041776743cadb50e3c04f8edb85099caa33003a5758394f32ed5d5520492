mply create a file `postinstall/presets.js` in your addon:\n\n```js\nimport { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"f6dd58a9fa9f197bfb74d0b6c7d6b5d266f759b9","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"sbmodern":"dist/modern/index.js","repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v16.17.0+arm64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"16.17.0","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"dist/ts3.9/*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","jscodeshift":"^0.13.1","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.5.17-alpha.0_1679541088716_0.5860866440882833","host":"s3://npm-registry-packages"}},"7.0.0-rc.7":{"name":"@storybook/postinstall","version":"7.0.0-rc.7","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@7.0.0-rc.7","maintainers":[{"name":"shilman","email":"michael@lab80.co"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"}],"homepage":"https://github.com/storybookjs/storybook/tree/main/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"2d459bf2a47e5811b938d9cd8c0ad12a398024c1","tarball":"https://registry.npmjs.org/