pi.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"32441319e4b2550b1887ebf38bf228123ab1dfcd","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"sbmodern":"dist/modern/index.js","repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v12.22.1+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"12.22.1","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.4.0-alpha.0_1624517038570_0.5963052079634277","host":"s3://npm-registry-packages"}},"6.4.0-alpha.1":{"name":"@storybook/postinstall","version":"6.4.0-alpha.1","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.4.0-alpha.1","maintainers":[{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"shilman","email":"michael@lab80.co"}],"homepage":"https://github.com/storybookjs/storybook/tree/master/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"a80b7dd6a0fbc982eb4397310b18a392f6fd2ea8","tarball":"https://registry.npmjs.org/@storybook/postinstall/-/postinstall-6.4.0-alpha.1.tgz","fileCount":24,"integrity":"sha512-HtlyjoNypckYHmwxPpiZzJyH7QYZjk8ButibdVu8yIyWV44RvlUut0jmKQA4aYPNNsiPCcPDgL7kAIqY0Rt22w==","signatures":