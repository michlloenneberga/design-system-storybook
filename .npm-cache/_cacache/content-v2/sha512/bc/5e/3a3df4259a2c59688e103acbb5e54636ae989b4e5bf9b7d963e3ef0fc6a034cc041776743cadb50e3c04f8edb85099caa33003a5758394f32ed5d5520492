ns' }, { root, api });\n  return root.toSource();\n};\n```\n","funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"e1f8050f2461859cd560fc166f2dfe9d5c478a55","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"sbmodern":"dist/modern/index.js","repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v14.17.6+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"14.17.6","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"dist/ts3.9/*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.5.0-alpha.12_1641778813509_0.9633031693996081","host":"s3://npm-registry-packages"}},"6.4.10":{"name":"@storybook/postinstall","version":"6.4.10","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.4.10","maintainers":[{"name":"shilman","email":"michael@lab80.co"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"}],"homepage":"https://github.com/storybookjs/storybook/tree/main/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"378c30c2b64050bf1f4024874592dd54b4413981","tarball":"https://registry.npmjs.org/@storybook/postinstall/-/postinstall-6.4.10.tgz","fileCount":24,"integrity":"sha512-kW+c2PLLiPvucMJLerFxJwcX9BPjQEttyLBd7Y+Qmhw8YC21mKszfW1gBiX5RMvDUdEMp9LYZOtZzNZ7s+FVdw==","signatures":[{"sig":"MEYCIQCxfDb0nn7ORziQdpJdAifzBsaqwKgyFjrvokhRysjhiAIhAJ5La4/cU7U/8WIETq2UoL23RASrZidIoYpopJ10KKDZ","keyid