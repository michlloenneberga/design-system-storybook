a preset\n\nTo add a preset to `presets.js`, simply create a file `postinstall/presets.js` in your addon:\n\n```js\nimport { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","bundler":{"entries":["./src/index.ts"]},"exports":{".":{"node":"./dist/index.js","types":"./dist/index.d.ts","import":"./dist/index.mjs","require":"./dist/index.js"},"./package.json":"./package.json"},"funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"f70dcdb599363bfd6e6d2b63f8fadd02e9cbe00a","scripts":{"prep":"../../../scripts/prepare/bundle.ts","check":"../../../scripts/node_modules/.bin/tsc --noEmit"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/6.5.1/node@v18.14.2+arm64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"18.14.2","publishConfig":{"access":"public"},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^29.3.1","typescript":"~4.9.3","jscodeshift":"^0.14.0","jest-specific-snapshot":"^7.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_7.0.0-beta.62_1678114544380_0.8725001743295904","host":"s3://npm-registry-packages"}},"7.0.0-beta.63":{"name":"@storybook/postinstall","version":"7.0.0-beta.63","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@7.0.0-beta.63","maintainers":[{"name":"shilman","email":"michael@lab80.co"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"}],"homepage":"https://github.com/sto