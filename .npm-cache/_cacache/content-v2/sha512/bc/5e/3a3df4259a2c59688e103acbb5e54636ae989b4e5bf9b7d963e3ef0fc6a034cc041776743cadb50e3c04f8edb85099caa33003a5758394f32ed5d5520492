addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"ca5ef7b7c83977f53fe73fe733c94b4695c6b0ec","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"sbmodern":"dist/modern/index.js","repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v16.2.0+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"16.2.0","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"dist/ts3.9/*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","jscodeshift":"^0.13.1","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.5.4-alpha.0_1653044171085_0.4795780934546259","host":"s3://npm-registry-packages"}},"6.5.4":{"name":"@storybook/postinstall","version":"6.5.4","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.5.4","maintainers":[{"name":"shilman","email":"michael@lab80.co"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"}],"homepage":"https://github.com/storybookjs/storybook/tree/main/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"01f7792f1a777cb1dee30e07f02314080cabd232","tarball":"https://registry.npmjs.org/@storybook/postinstall/-/postinstall-6.5.4.tgz","fileCount":24,"integrity":"sha512-euiZOwz5KmtL3wcSxzxLTkGkxLCroZTVaNt/NopRBa0F8erJfT9MiQPVw/xFkS9QwEViaR+Zq4KmStxB41zycQ==","signatures":[{"sig":"MEQCIBlMR6XKqg+zI6Ruq9svViZO2xRT54unF697Z4iUz8AAAiBdzzCMT6TaON1KG046JPJXQ/fhMi9i9GmhOuD