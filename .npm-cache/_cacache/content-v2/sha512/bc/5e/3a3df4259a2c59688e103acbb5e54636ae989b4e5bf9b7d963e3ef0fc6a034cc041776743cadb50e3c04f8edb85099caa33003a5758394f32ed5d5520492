```js\nimport { presetsAddPreset } = require('@storybook/postinstall');\nexport default function transformer(file, api) {\n  const root = api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"ce3592fc323ede3d33939b532ff7400cfdc1efa1","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"sbmodern":"dist/modern/index.js","repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v14.17.6+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"14.17.6","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"dist/ts3.9/*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.5.0-alpha.19_1642420743519_0.7662103344472591","host":"s3://npm-registry-packages"}},"6.5.0-alpha.20":{"name":"@storybook/postinstall","version":"6.5.0-alpha.20","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.5.0-alpha.20","maintainers":[{"name":"shilman","email":"michael@lab80.co"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"igor-dv","email":"davydkin.igor@gmail.com"}],"homepage":"https://github.com/storybookjs/storybook/tree/main/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"adcf2d819058a7e06329d725c6dcd22dc5f37c35","tarball":"https://registry.npmjs.org/@storybook/postinstall/-/postinstall-6.5.0