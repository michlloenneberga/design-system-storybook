= api.jscodeshift(file.source);\n  presetsAddPreset(`@storybook/addon-docs/preset`, { some: 'options' }, { root, api });\n  return root.toSource();\n};\n```\n","funding":{"url":"https://opencollective.com/storybook","type":"opencollective"},"gitHead":"027a5cfedb93342ef23f8cbdc891b97ea1b5f763","scripts":{"prepare":"node ../../scripts/prepare.js"},"_npmUser":{"name":"shilman","email":"michael@lab80.co"},"sbmodern":"dist/modern/index.js","repository":{"url":"git+https://github.com/storybookjs/storybook.git","type":"git","directory":"lib/postinstall"},"_npmVersion":"lerna/3.22.1/node@v12.22.1+x64 (darwin)","description":"Storybook addons postinstall utilities","directories":{},"sideEffects":false,"_nodeVersion":"12.22.1","dependencies":{"core-js":"^3.8.2"},"publishConfig":{"access":"public"},"typesVersions":{"<3.8":{"*":["dist/ts3.4/*"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"jest":"^26.6.3","@hypnosphi/jscodeshift":"^0.6.4","jest-specific-snapshot":"^4.0.0"},"_npmOperationalInternal":{"tmp":"tmp/postinstall_6.4.0-alpha.5_1624905001597_0.3378928447684224","host":"s3://npm-registry-packages"}},"6.4.0-alpha.6":{"name":"@storybook/postinstall","version":"6.4.0-alpha.6","keywords":["api","storybook"],"license":"MIT","_id":"@storybook/postinstall@6.4.0-alpha.6","maintainers":[{"name":"igor-dv","email":"davydkin.igor@gmail.com"},{"name":"tmeasday","email":"tom@thesnail.org"},{"name":"hypnosphi","email":"talpa@yandex.ru"},{"name":"ndelangen","email":"ndelangen@me.com"},{"name":"shilman","email":"michael@lab80.co"}],"homepage":"https://github.com/storybookjs/storybook/tree/master/lib/postinstall","bugs":{"url":"https://github.com/storybookjs/storybook/issues"},"dist":{"shasum":"31156419af53d1af865116ce53b1d5105edfb601","tarball":"https://registry.npmjs.org/@storybook/postinstall/-/postinstall-6.4.0-alpha.6.tgz","fileCount":24,"integrity":"sha512-wVKZKgY59yOqcrb4N84MrE+sODqUI0tShQbl9Ab+dmvAP9uQJRQRcCv+w7liUhXXJw++f62YKWieDkAkimPcGg==","signature